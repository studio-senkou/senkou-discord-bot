{
  "name": "senkou-discord-bot",
  "version": "1.0.0",
  "description": "Discord Bot Server for Senkou Studio",
  "type": "module",
  "main": "./src/main.ts",
  "scripts": {
    "start": "bun run ./src/main.ts",
    "start:prod": "bun run ./dist/main.js",
    "dev": "bun run --watch ./src/main.ts",
    "build": "bunx tsc",
    "build:bun": "bun build ./src/main.ts --outdir ./dist --target bun",
    "clean": "rm -rf ./dist node_modules/.cache",
    "test": "bun test",
    "analyze": "bun build ./src/main.ts --analyze --target bun"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studio-senkou/senkou-discord-bot.git"
  },
  "keywords": [
    "Senkou",
    "Discord",
    "Bot"
  ],
  "author": "Aji Hamdani Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/studio-senkou/senkou-discord-bot/issues"
  },
  "homepage": "https://github.com/studio-senkou/senkou-discord-bot#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "auto-bind": "^5.0.1",
    "body-parser": "^2.2.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.1",
    "typescript": "^5.8.3"
  }
}
